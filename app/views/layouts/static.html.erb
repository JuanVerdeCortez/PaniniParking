    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="wrapper">
      <%= render 'layouts/navigation/header'%>
      <%= render 'layouts/navigation/sidebar'%>
